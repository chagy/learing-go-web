{{template "base" .}}

{{define "content"}}
<div class="container">
    <div class="row">
        <div class="col">
            <h1>This is about page {{index .StringMap "test"}}</h1>
            <p>
                {{if ne (index .StringMap "remote_ip") "" }}
                    Your remote ip address is {{index .StringMap "remote_ip"}}
                {{else}}
                    I don't know ip address go to <a href="/">Home Page</a>
                {{end}}
            </p>
        </div>
    </div>
</div>
{{end}}